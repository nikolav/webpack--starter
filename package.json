{
  "name": "app",
  "version": "0.0.3",
  "main": "index.js",
  "author": "nikolav",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"webpack serve --mode=development\" \"yarn run watch:css\"",
    "build": "yarn run build:css && cross-env NODE_ENV=production webpack --mode=production",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*.js ",
    "build:dev": "yarn run build:css && webpack --mode=development",
    "watch": "webpack --watch",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./src/style/build.css",
    "watch:css": "tailwindcss -i ./src/tailwind.css -o ./src/style/build.css --watch",
    "docs": "serve -p 8888 docs"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@webpack-cli/generators": "^2.5.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "html5-boilerplate": "^8.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "2.7.1",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "d3": "^7.6.1",
    "nikolav-q": "^1.1.7"
  }
}
